{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Effect": "Allow",
          "Action": [
              "s3:GetReplicationConfiguration",
              "s3:ListBucket"
          ],
          "Resource": [
              "arn:aws:s3:::replication-fun-ab-4535"
          ]
      },
      {
          "Effect": "Allow",
          "Action": [
              "s3:GetObjectVersion",
              "s3:GetObjectVersionAcl",
              "s3:GetObjectVersionTagging"
          ],
          "Resource": [
              "arn:aws:s3:::replication-fun-ab-4535/*"
          ]
      },
      {
          "Effect": "Allow",
          "Action": [
              "s3:ReplicateObject",
              "s3:ReplicateDelete",
              "s3:ReplicateTags"
          ],
          "Resource": "arn:aws:s3:::replication-fun-us-east-ab-4535/*"
      }
  ]
}